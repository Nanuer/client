<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MainPont">
        <item name="android:fontFamily">@font/noto_sans_kr_medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">19sp</item>
    </style>
    <style name="BtnPont">
        <item name="android:fontFamily">@font/noto_sans_kr_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="MidPont">
        <item name="android:fontFamily">@font/noto_sans_kr_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="MinPont">
        <item name="android:fontFamily">@font/noto_sans_kr_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">15sp</item>
        <item name="android:letterSpacing">-0.03</item>
    </style>
    <style name="BoldPont">
        <item name="android:fontFamily">@font/noto_sans_display_medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="Tip">
        <item name="android:fontFamily">@font/noto_sans_medium</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="BtnPontSmall">
        <item name="android:fontFamily">@font/noto_sans_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">11.3sp</item>
    </style>
</resources>